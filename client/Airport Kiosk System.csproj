<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <OutputType>WinExe</OutputType>
    <TargetFramework>net9.0-windows</TargetFramework>
    <RootNamespace>Airport_Kiosk_System</RootNamespace>
    <Nullable>enable</Nullable>
    <ImplicitUsings>enable</ImplicitUsings>
    <UseWPF>true</UseWPF>
  </PropertyGroup>

  <ItemGroup>
    <None Remove="fonts\NotoSans-Italic-VariableFont_wdth,wght.ttf" />
    <None Remove="fonts\NotoSans-VariableFont_wdth,wght.ttf" />
    <None Remove="images\InitScreenImage.jpeg" />
  </ItemGroup>

  <ItemGroup>
    <Resource Include="fonts\NotoSans-Italic-VariableFont_wdth,wght.ttf" />
    <Resource Include="fonts\NotoSans-VariableFont_wdth,wght.ttf">
      <CopyToOutputDirectory></CopyToOutputDirectory>
    </Resource>
    <Resource Include="images\InitScreenImage.jpeg" />
  </ItemGroup>

  <ItemGroup>
    <Folder Include="models\" />
  </ItemGroup>

</Project>
